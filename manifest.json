{
    "name": "browser extension",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "content_scripts":[
    	{
    	"matches" : [
    		"<all_urls>"
    	],
    	"js":["content.js"],
    	"persistant":false
    	}
    ],
    	"background":{
    		"scripts":["background.js"]
    	},
    	"browser_action":{
    		"default_icon":"hello_extensions.png"
    	}
}